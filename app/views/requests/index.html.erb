<h1>Listing requests</h1>
<table>
  <tr>
    <th>Requester</th>
    <th>Angel</th>
    <th>Obo</th>
    <th>Anon req</th>
    <th>Anon angel</th>
    <th>Ship address</th>
    <th>Ship city</th>
    <th>Ship state</th>
    <th>Ship zipcode</th>
    <th>Story</th>
    <th>External url</th>
    <th>Before photo url</th>
    <th>After photo url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
  <% @requests.each do |request| %>
    <tr>
      <td>
        <%= request.requestor_id %>
      </td>
      <td>
        <%= request.angel_id %>
      </td>
      <td>
        <%= request.obo %>
      </td>
      <td>
        <%= request.anon_req %>
      </td>
      <td>
        <%= request.anon_angel %>
      </td>
      <td>
        <%= request.ship_address %>
      </td>
      <td>
        <%= request.ship_city %>
      </td>
      <td>
        <%= request.ship_state %>
      </td>
      <td>
        <%= request.ship_zipcode %>
      </td>
      <td>
        <%= request.story %>
      </td>
      <td>
        <%= request.external_url %>
      </td>
      <td>
        <%= request.before_photo_url %>
      </td>
      <td>
        <%= request.after_photo_url %>
      </td>
      <td>
        <%= link_to 'Show', request %>
      </td>
      <td>
        <%= link_to 'Edit', edit_request_path(request) %>
      </td>
      <td>
        <%= link_to 'Destroy', request, :method => :delete, :data => { :confirm => 'Are you sure?' } %>
      </td>
    </tr>
  <% end %>
</table>
<br/>
<%= link_to 'New Request', new_request_path %>
