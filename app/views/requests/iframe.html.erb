    
<script src="//http://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
<%= stylesheet_link_tag "application", :media => "all" %>
<%= javascript_include_tag "application" %>

<div id="iframe_requests">
  <% @requests.each_with_index do |req, index| %>
    <div id="request_image">
      <%= image_tag(req.before_photo_url, size: "300x300", :class => "rimage", :data => { :story => req.story }) %>
    </div>
  <% end %>
</div>

<div id="iframe_slogan">
  <h1>Give A SMAC!</h1>
</div>  

<div id="story">
  <h1>Todd is a genious.</h1>
</div>
  