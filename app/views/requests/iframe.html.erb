    
<script src="//http://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
<%= stylesheet_link_tag "application", :media => "all" %>
<%= javascript_include_tag "application" %>

<div id="iframe_requests" class="row">
  <div id="request_image" class="small-8 columns">
  <% @requests.each_with_index do |req, index| %>
      <%= image_tag(req.before_photo_url, size: "150x150", :class => "rimage", :data => { :story => req.story }) %>
  <% end %>
  </div>
</div>

<div id = "logo_story" class="small-4 columns">
  <div id="iframe_image">
     <%= image_tag "/assets/SMAC_logo.png", size: "120x120" %>
   </div>

   <div id="iframe_slogan">
     <h1>Give A SMAC!</h1>
   </div>  

   <div id="story">
     <h1>Todd is a genious.</h1>
   </div>
</div>
